public class dtnamcimethods {
    public static void methodforacounts(){
        string tharun ='SELECT Id, Name FROM Account';
        list<Account> acc1 =Database.query(tharun);
        for(Account acc2:acc1){
            system.debug(acc2.Name);
        }
    }

}
